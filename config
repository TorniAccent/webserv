server
{
	host 0.0.0.0;
	port 1700;
	server_name web.com;
	index index.html index.htm index.php;
	limit 4096;
	error_pages /error_pages/;

	location /dir/index.php:
	{
		root /root/;
		methods GET POST DELETE;
		autoindex off;
		index max.php tar.php;
		cgi /root/phpmyadmin/;
	}

	location /dir/index.php:
	{
		root /root/;
		methods GET POST DELETE;
		autoindex off;
		index max.php tar.php;
		cgi /root/phpmyadmin/;
	}
}

