    tmp/1/2/favicon.ico
    tmp/1/2/index.php

	location /dir/:
	{
		root /root/;
		methods GET POST DELETE;
		autoindex off;
		index max.php tar.php;
		cgi /root/phpmyadmin/;
	}

	location /dir/index.php:
	{
		root /root/;
		methods GET POST DELETE;
		autoindex off;
		index max.php tar.php;
		cgi /root/phpmyadmin/;
	}
}